{
  "$res": {
    "@func": "apijson",
    "datasource": "default",
    "method": "get",
    "request": {
      "[]": {
        "API_FILTER":{
          "@column": "id,parent_type,parent_id,title,implement_type,point,status,gmt_create,gmt_modified",
          "@order": "sort"
        },
        "query": 2
      },
      "total@": "/[]/total",
      "info@": "/[]/info"
    }
  },
  "*": {
    "@": "@{$res}",
    "$":{
      "success_code" : 200
    },
    "msg": "@{:msg}",
    "detail": "@{cast(cast(data('$'),'JSONString'),'JSONObject')}",
    "*": {
      "@func": "@if",
      "select": "@{data.code == vars.success_code}",
      "true": {
        "data": {
          "rows": "@data('API_FILTER\\\\[\\\\]');",
          "total": "@",
          "info": "@"
        },
        "status": 0
      },
      "false": {
        "status": "@{:code}"
      }
    }
  }
}