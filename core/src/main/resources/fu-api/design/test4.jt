{
  "$res": {
    "@func": "apijson",
    "datasource": "default",
    "method": "get",
    "request": {
      "[]": {
        "API_FILTER": {
          "@column": "id,parent_type,parent_id,title,implement_type,point,status,gmt_create,gmt_modified",
          "@order": "sort"
        },
        "query": 2
      },
      "total@": "/[]/total",
      "info@": "/[]/info"
    }
  },
  "*": {
    "@func": "@if",
    "select": "@{ self.res.code == 200 }",
    "true":{
      "data": {
        "rows": "@(vars.'res'.'[]'.'API_FILTER');",
        "total": "@(vars.res.total);",
        "info": "@(vars.res.info);"
      },
       "status": 0
    },
    "false": {
      "status": "@(vars.res.code);"
    }
  },
  "msg": "@(vars.res.msg);"
}