{
  "$res": {
    "@func": "apijson",
    "datasource": "default",
    "method": "get",
    "request": {
      "[]": {
        "API_FILTER": {
          "@column": "id,parent_type,parent_id,title,implement_type,point,status,gmt_create,gmt_modified",
          "@order": "sort"
        },
        "query": 2
      },
      "total@": "/[]/total",
      "info@": "/[]/info"
    },
    "#template": {
      "data": {
        "rows": "@data('\\\\[\\\\].API_FILTER');",
        "total": "@data('total');",
        "info": "@data('info');"
      },
      "code": "@data('code');",
      "msg": "@data('msg');"
    },
    "template": "@vars('template');"
  },
  "*": {
    "@func": "@if",
    "select": "@(vars('res').code == 200);",
    "true":{
      "data": "@(vars('res').data);",
      "msg": "@(vars('res').msg);",
      "status": 0
    },
    "false": {
      "status": "@(vars('res').code);",
      "msg": "@(vars('res').msg);"
    }
  }

}