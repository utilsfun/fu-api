{
  "type": "wrapper",
  "size": "none",
  "body": {
    "type": "service",
    "data": "@{$}",
    "body": {
      "type": "collapse",
      "headingClassName": "font-bold no-border b-info text-base text-info",
      "body": [
        {
          "type": "tpl",
          "tpl": "@{note}",
          "inline": false
        },
        {
          "type": "html",
          "html": "@{content}"
        }
      ],
      "title": "@{title}"
    }
  }
}
